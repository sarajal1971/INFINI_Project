#define TIME_TLE82453_RESET_LOW     100     /* 100ms */
#define TIME_TLE82453_RESET_HIGH    100     /* 100ms */
#define TIME_TLE82453_RETRY         100     /* 100ms */
#define TIME_TEST_PWM_INTERVAL      20      /* 20ms */
